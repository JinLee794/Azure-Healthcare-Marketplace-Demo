{
  "resourceType": "Claim",
  "id": "pa-request-template",
  "meta": {
    "profile": [
      "http://hl7.org/fhir/us/davinci-pas/StructureDefinition/profile-claim"
    ]
  },
  "status": "active",
  "type": {
    "coding": [
      {
        "system": "http://terminology.hl7.org/CodeSystem/claim-type",
        "code": "professional",
        "display": "Professional"
      }
    ]
  },
  "use": "preauthorization",
  "patient": {
    "reference": "Patient/{{PATIENT_ID}}"
  },
  "created": "{{CREATED_DATE}}",
  "insurer": {
    "reference": "Organization/{{PAYER_ID}}"
  },
  "provider": {
    "reference": "Organization/{{PROVIDER_ID}}"
  },
  "priority": {
    "coding": [
      {
        "system": "http://terminology.hl7.org/CodeSystem/processpriority",
        "code": "normal"
      }
    ]
  },
  "insurance": [
    {
      "sequence": 1,
      "focal": true,
      "coverage": {
        "reference": "Coverage/{{COVERAGE_ID}}"
      }
    }
  ],
  "diagnosis": [
    {
      "sequence": 1,
      "diagnosisCodeableConcept": {
        "coding": [
          {
            "system": "http://hl7.org/fhir/sid/icd-10-cm",
            "code": "{{ICD10_CODE}}",
            "display": "{{ICD10_DISPLAY}}"
          }
        ]
      }
    }
  ],
  "procedure": [
    {
      "sequence": 1,
      "procedureCodeableConcept": {
        "coding": [
          {
            "system": "http://www.ama-assn.org/go/cpt",
            "code": "{{CPT_CODE}}",
            "display": "{{CPT_DISPLAY}}"
          }
        ]
      }
    }
  ],
  "item": [
    {
      "sequence": 1,
      "productOrService": {
        "coding": [
          {
            "system": "http://www.ama-assn.org/go/cpt",
            "code": "{{CPT_CODE}}",
            "display": "{{CPT_DISPLAY}}"
          }
        ]
      },
      "servicedPeriod": {
        "start": "{{SERVICE_START_DATE}}",
        "end": "{{SERVICE_END_DATE}}"
      },
      "locationCodeableConcept": {
        "coding": [
          {
            "system": "https://www.cms.gov/Medicare/Coding/place-of-service-codes",
            "code": "{{PLACE_OF_SERVICE}}",
            "display": "{{PLACE_OF_SERVICE_DISPLAY}}"
          }
        ]
      },
      "quantity": {
        "value": 1
      }
    }
  ],
  "supportingInfo": [
    {
      "sequence": 1,
      "category": {
        "coding": [
          {
            "system": "http://hl7.org/fhir/us/davinci-pas/CodeSystem/PASSupportingInfoType",
            "code": "freeformtext"
          }
        ]
      },
      "valueString": "{{CLINICAL_NOTES}}"
    }
  ]
}
